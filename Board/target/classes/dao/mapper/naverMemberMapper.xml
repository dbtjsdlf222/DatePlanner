<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="dao.INmemberDao">
  
  <select id="selectOne" parameterType="java.util.HashMap" resultType="java.util.HashMap">
  select * from naverMember where id = #{id}
  </select>
  
  <insert id="insertMember" parameterType="java.util.HashMap">
  insert into naverMember values(#{id},#{name},#{nickName},#{email})
  </insert>
  
  </mapper>